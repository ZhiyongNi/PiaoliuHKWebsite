<body>
<form action="packagelistform" method="post" target="_new" id="packagelistform">
  仓库中未匹配包裹
  <?php if ($this->PackageList): ?>
  <table width="800" border="1">
    <tr>
      <th scope="col">&nbsp;</th>
      <th scope="col">编号</th>
      <th scope="col">收货人</th>
      <th scope="col">手机号</th>
      <th scope="col">快递公司</th>
      <th scope="col">运单号</th>
      <th scope="col">照片</th>
      <th scope="col">重量</th>
      <th scope="col">运费</th>
      <th scope="col">入库时间</th>
      <th scope="col">出库时间</th>
      <th scope="col">状态</th>
    </tr>
    <?php foreach ($this->PackageList as $Value): ?>
    <tr>
      <th scope="row"><input type="radio" name="PackageIDR" id="PackageIDR" value="<?php echo $Value["PackageID"] ?>" /></th>
      <td><?php echo $Value["PackageID"] ?></td>
      <td><?php echo $Value["PackageOwnerID"] ?></td>
      <td><?php echo $Value["PackageOwnerMobile"] ?></td>
      <td><?php echo $Value["PackageExpressCompany"] ?></td>
      <td><?php echo $Value["PackageExpressTrackNumber"] ?></td>
      <td><?php echo $Value["PackageSnapshot"] ?></td>
      <td><?php echo $Value["PackageWeight"] ?></td>
      <td><?php echo $Value["PackageFare"] ?></td>
      <td><?php echo $Value["PackageInTime"] ?></td>
      <td><?php echo $Value["PackageOutTime"] ?></td>
      <td><?php echo $Value["PackageStatus"] ?></td>
    </tr>
    <?php endforeach; ?>
  </table>
  <?php else: ?>
  <p>There are no books to display.</p>
  <?php endif; ?>
  <input name="detail" type="submit" id="detail" value="详情" />
  <input name="close" type="button" id="close" value="关闭" />
